{
  "kind": "collectionType",
  "collectionName": "templates",
  "info": {
    "singularName": "template",
    "pluralName": "templates",
    "displayName": "Template",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "price": {
      "type": "string",
      "required": true
    },
    "thumbnail": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "numberForTemplatePosition": {
      "type": "integer",
      "unique": true
    },
    "tagLine": {
      "type": "string",
      "required": true
    },
    "sliderThumbnail": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "previewLiveDemoURL": {
      "type": "string",
      "required": true,
      "regex": "^https?:\\/\\/(www\\.)?[-a-zA-Z0-9@:%._\\+~#=]{1,256}\\.[a-zA-Z0-9()]{1,6}\\b([-a-zA-Z0-9()@:%_\\+.~#?&//=]*)$"
    },
    "getThisTemplateURL": {
      "type": "string",
      "required": true,
      "regex": "^https?:\\/\\/(www\\.)?[-a-zA-Z0-9@:%._\\+~#=]{1,256}\\.[a-zA-Z0-9()]{1,6}\\b([-a-zA-Z0-9()@:%_\\+.~#?&//=]*)$"
    },
    "content": {
      "type": "blocks",
      "required": true
    },
    "pageInclude": {
      "type": "blocks",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "name",
      "options": {
        "separator": "-",
        "lowercase": true,
        "decamelize": false
      }
    },
    "feature": {
      "type": "boolean",
      "default": false
    }
  }
}
